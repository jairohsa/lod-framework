@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix poa: <http://planificacion.org/poa#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix qb: <http://purl.org/linked-data/cube#> .


<http://planificacion.org/poa> rdf:type owl:Ontology .

<http://www.geonames.org/ontology#Feature> rdf:type owl:Class ;
  rdfs:label "Feature"@en ;
  rdfs:comment "A geographical feature"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://planificacion.org/poa#belongsToOperationalActivity
poa:belongsToOperationalActivity rdf:type owl:ObjectProperty ;
                                 owl:inverseOf poa:hasBudgetedActivity ;
                                 rdfs:domain poa:BudgetedActivity ;
                                 rdfs:range poa:OperationalActivity ;
                                 rdfs:label "pertenece a actividad operativa"@es , "belongs to operational activity"@en ;
                                 skos:prefLabel "pertenece a actividad operativa"@es , "belongs to operational activity"@en .


###  http://planificacion.org/poa#belongsToPoa
poa:belongsToPoa rdf:type owl:ObjectProperty ;
                 rdfs:domain poa:Program ;
                 rdfs:range poa:AnnualOperationalPlan ;
                 rdfs:label "pertenece al POA"@es , "belongs to POA"@en ;
                 skos:prefLabel "pertenece al POA"@es , "belongs to POA"@en .


###  http://planificacion.org/poa#belongsToProgram
poa:belongsToProgram rdf:type owl:ObjectProperty ;
                     owl:inverseOf poa:hasSubprogram ;
                     rdfs:domain poa:Subprogram ;
                     rdfs:range poa:Program ;
                     rdfs:label "pertenece al Programa"@es , "belongs to Program"@en ;
                     skos:prefLabel "pertenece al Programa"@es , "belongs to Program"@en.



###  http://planificacion.org/poa#belongsToProject
poa:belongsToProject rdf:type owl:ObjectProperty ;
                     owl:inverseOf poa:hasOperationalActivity ;
                     rdfs:domain poa:OperationalActivity;
                     rdfs:range poa:Project ;
		     rdfs:label "pertenece a proyecto"@es , "belongs to Proyect"@en ;
    		     skos:prefLabel "pertenece a proyecto"@es , "belongs to Proyect"@en .

###  http://planificacion.org/poa#belongsToSubprogram
poa:belongsToSubprogram rdf:type owl:ObjectProperty ;
                        owl:inverseOf poa:hasProject ;
                        rdfs:domain poa:Project ;
                        rdfs:range poa:Subprogram ;
                        rdfs:label "pertenece al SubPrograma"@es , "belongs to SubProgram"@en ;
                        skos:prefLabel "pertenece al SubPrograma"@es , "belongs to SubProgram"@en.



###  http://planificacion.org/poa#hasBudgetedActivity
poa:hasBudgetedActivity rdf:type owl:ObjectProperty ;
                        rdfs:domain poa:OperationalActivity ;
                        rdfs:range poa:BudgetedActivity .


### http://planificacion.org/poa#hasCanton
poa:hasCanton rdf:type owl:ObjectProperty ;
  rdfs:domain poa:OperationalActivity ;
  rdfs:range <http://www.geonames.org/ontology#Feature> ;
  rdfs:label "has canton"@en , "tiene cantón"@es ;
  skos:prefLabel "has canton"@en , "tiene cantón"@es ;
  rdfs:comment "Enlace a la URI de GeoNames que representa un cantón."@es ,
               "Link to the GeoNames URI representing a canton."@en .



###  http://planificacion.org/poa#hasGoal
poa:hasGoal rdf:type owl:ObjectProperty ;
            rdfs:domain poa:Project ;
            rdfs:range poa:Goal ;
            rdfs:label "tiene meta"@es ;
	    rdfs:label "has goal"@en ;
	    skos:prefLabel "tiene meta"@es ;
	    skos:prefLabel "has goal"@en .


###  http://planificacion.org/poa#hasOperationalActivity
poa:hasOperationalActivity rdf:type owl:ObjectProperty ;
                           rdfs:domain poa:Project ;
                           rdfs:range poa:OperationalActivity;
                           rdfs:label "tiene actividad operativa"@es , "has operational activity"@en ;
                           skos:prefLabel "tiene actividad operativa"@es , "has operational activity"@en .


### http://planificacion.org/poa#hasParish
poa:hasParish rdf:type owl:ObjectProperty ;
  rdfs:domain poa:OperationalActivity;
  rdfs:range <http://www.geonames.org/ontology#Feature> ;
  rdfs:label "has parish"@en , "tiene parroquia"@es ;
  skos:prefLabel "has parish"@en , "tiene parroquia"@es ;
  rdfs:comment "Enlace a la URI de GeoNames que representa una parroquia."@es ,
               "Link to the GeoNames URI representing a parish."@en .

### http://planificacion.org/poa#locatedInProvince
poa:locatedInProvince rdf:type owl:ObjectProperty ;
    rdfs:domain poa:Location ;
    rdfs:range owl:Thing ;
    rdfs:label "está en provincia"@es ;
    skos:prefLabel "está en provincia"@es ;
    rdfs:label "located in province"@en ;
    skos:prefLabel "located in province"@en ;
    rdfs:comment "Enlaza una ubicación con la provincia correspondiente (GeoNames URI)."@es ,
                 "Links a location with its corresponding province (GeoNames URI)."@en .

### http://planificacion.org/poa#locatedInCanton
poa:locatedInCanton rdf:type owl:ObjectProperty ;
    rdfs:domain poa:Location ;
    rdfs:range owl:Thing ;
    rdfs:label "está en cantón"@es ;
    skos:prefLabel "está en cantón"@es ;
    rdfs:label "located in canton"@en ;
    skos:prefLabel "located in canton"@en ;
    rdfs:comment "Enlaza una ubicación con el cantón correspondiente (GeoNames URI)."@es ,
                 "Links a location with its corresponding canton (GeoNames URI)."@en .

### http://planificacion.org/poa#locatedInParish
poa:locatedInParish rdf:type owl:ObjectProperty ;
    rdfs:domain poa:Location ;
    rdfs:range owl:Thing ;
    rdfs:label "está en parroquia"@es ;
    skos:prefLabel "está en parroquia"@es ;
    rdfs:label "located in parish"@en ;
    skos:prefLabel "located in parish"@en ;
    rdfs:comment "Enlaza una ubicación con la parroquia correspondiente (GeoNames URI)."@es ,
                 "Links a location with its corresponding parish (GeoNames URI)."@en .

###  http://planificacion.org/poa#hasProgram
poa:hasProgram rdf:type owl:ObjectProperty ;
               rdfs:domain poa:AnnualOperationalPlan ;
               rdfs:range poa:Program ;
               rdfs:label "tiene programa"@es, "has program"@en ;
               skos:prefLabel  "tiene programa"@es, "has program"@en .


###  http://planificacion.org/poa#hasProject
poa:hasProject rdf:type owl:ObjectProperty ;
               rdfs:domain poa:Subprogram ;
               rdfs:range poa:Project ;
               rdfs:label "tiene proyecto"@es, "has project"@en ;
               skos:prefLabel  "tiene proyecto"@es, "has project"@en .


### http://planificacion.org/poa#hasProvince
poa:hasProvince rdf:type owl:ObjectProperty ;
  rdfs:domain poa:OperationalActivity ;
  rdfs:range <http://www.geonames.org/ontology#Feature> ;
  rdfs:label "has province"@en , "tiene provincia"@es ;
  skos:prefLabel "has province"@en , "tiene provincia"@es ;
  rdfs:comment "Enlace a la URI de GeoNames que representa una provincia."@es ,
               "Link to the GeoNames URI representing a province."@en .

###  http://planificacion.org/poa#hasSubprogram
poa:hasSubprogram rdf:type owl:ObjectProperty ;
                  rdfs:domain poa:Program ;
                  rdfs:range poa:Subprogram ;
                  rdfs:label "tiene subprograma"@es ;
                  rdfs:label "has subprogram"@en ;
                  skos:prefLabel "tiene subprograma"@es ;
                  skos:prefLabel "has subprogram"@en .


###  http://planificacion.org/poa#hasTemporalScope
poa:hasTemporalScope rdf:type owl:ObjectProperty ;
                     rdfs:domain poa:Project ;
                     rdfs:range skos:Concept .


###  http://planificacion.org/poa#hasWorkType
poa:hasWorkType rdf:type owl:ObjectProperty ;
  rdfs:domain poa:BudgetedActivity ;
  rdfs:range skos:Concept ;
  rdfs:label "tiene tipo de obra"@es ;
  skos:prefLabel "tiene tipo de obra"@es ;
  rdfs:label "has work type"@en ;
  skos:prefLabel "has work type"@en .


###  http://planificacion.org/poa#unitOfMeasure
poa:unitOfMeasure rdf:type owl:ObjectProperty ;
                  rdfs:domain poa:Goal ;
                  rdfs:range poa:MeasurementUnit ;
                  rdfs:label "Unidad de medida"@es , "Unit of measure"@en ;
                  skos:prefLabel "Unidad de medida"@es , "Unit of measure"@en .


###  http://planificacion.org/poa#hasFundingSource
poa:hasFundingSource rdf:type owl:ObjectProperty ;
                      rdfs:domain poa:BudgetedActivity ;
    rdfs:range poa:FundingSource ;
    rdfs:label "tiene fuente de financiamiento"@es ;
    rdfs:label "has funding source"@en;
    skos:prefLabel "tiene fuente de financiamiento"@es ;
    skos:prefLabel "has funding source"@en .

###  http://planificacion.org/poa#hasLocation
poa:hasLocation rdf:type owl:ObjectProperty ;
    rdfs:domain poa:BudgetedActivity ;
    rdfs:range poa:Location ;
    rdfs:label "has location"@en ;
    skos:prefLabel "has location"@en ;
    rdfs:label "tiene ubicación"@es ;
    skos:prefLabel "tiene ubicación"@es ;
    rdfs:comment "Relaciona una actividad presupuestaria con su ubicación geográfica estructurada."@es ,
                 "Relates a budgeted activity with its structured geographic location."@en .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty ;
                rdfs:domain poa:AnnualOperationalPlan ;
                rdfs:range foaf:Organization ;
                rdfs:label "creador"@es , "creator"@en ;
                skos:prefLabel "creador"@es , "creator"@en ;
                rdfs:comment "Organización que elabora el Plan Operativo Anual."@es ,
                "Organization that created the Annual Operational Plan."@en .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:ObjectProperty ;
             rdfs:comment "Proporciona un enlace a otro recurso informativo o complementario."@es ;
             rdfs:label "see also"@en ,
                        "ver también"@es .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:ObjectProperty ;
           rdfs:comment "Indica que este recurso es semánticamente idéntico a uno externo."@es ;
           rdfs:label "es el mismo que"@es ,
                      "is the same as"@en .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:ObjectProperty ;
                rdfs:comment "Concepto similar, pero no estrictamente igual."@es ;
                rdfs:label "close match"@en ,
                           "coincide aproximadamente con"@es .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:ObjectProperty ;
                rdfs:comment "Indica una correspondencia semántica total entre conceptos."@es ;
                rdfs:label "coincide exactamente con"@es ,
                           "exact match"@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty ;
              rdfs:domain poa:MeasurementUnit.




