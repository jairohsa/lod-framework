###################################################################
# CubeViz
###################################################################

## Declara el conjunto de datos ##
poa:POA2025 a qb:DataSet ;
    rdfs:label "Plan Operativo Anual 2025"@es ;
    dcterms:type poa:AnnualOperationalPlan ;
    qb:structure poa:POAStructure .

### http://planificacion.org/poa#observesBudgetedActivity
poa:observesBudgetedActivity rdf:type owl:ObjectProperty ;
    rdfs:domain qb:Observation ;
    rdfs:range poa:BudgetedActivity ;
    rdfs:label "observa actividad presupuestada"@es , "observes budgeted activity"@en ;
    skos:prefLabel "observa actividad presupuestada"@es , "observes budgeted activity"@en ;
    rdfs:comment "Relaciona una observación con la actividad presupuestada a la que se refiere."@es ,
                 "Links an observation to the budgeted activity it refers to."@en .


poa:ComponentObservedActivity a qb:ComponentSpecification ;
    qb:dimension poa:observesBudgetedActivity ;
    rdfs:label "Actividad presupuestada observada"@es ;
    rdfs:label "Observed budgeted activity"@en ;
    rdfs:comment "Vínculo desde una observación hacia la actividad presupuestaria que representa."@es .


###########################################################################
##### Declaran qué propiedades del modelo se usan como ejes o métricas ####
###########################################################################

poa:fundingSourceAmount a qb:MeasureProperty ;
    rdfs:label "Monto de la fuente de financiamiento"@es ;
    rdfs:label "Funding source amount"@en ;
    rdfs:range xsd:decimal .

poa:ComponentAmount a qb:ComponentSpecification ;
    rdfs:label "Monto de la fuente de financiamiento"@es ;
    rdfs:label "Funding source amount"@en ;
    qb:measure poa:fundingSourceAmount ;
    rdfs:comment "Valor monetario asignado a la actividad presupuestaria en el POA."@es .

poa:ComponentCode a qb:ComponentSpecification ;
    rdfs:label "Código de actividad presupuestaria"@es ;
    rdfs:label "Budgeted activity code"@en;
    qb:dimension poa:hasCode ;
    rdfs:comment "Código que identifica a cada actividad presupuestaria dentro del POA."@es .

poa:ComponentProvince a qb:ComponentSpecification ;
    rdfs:label "Provincia"@es ;
    rdfs:label "Province"@en ;
    qb:dimension poa:hasProvince ;
    rdfs:comment "Provincia donde se ejecuta la actividad, usando URI de GeoNames."@es .

poa:ComponentCanton a qb:ComponentSpecification ;
    rdfs:label "Cantón"@es ;
    rdfs:label "Canton"@en ;
    qb:dimension poa:hasCanton ;
    rdfs:comment "Cantón donde se ejecuta la actividad, usando URI de GeoNames."@es .

poa:ComponentParish a qb:ComponentSpecification ;
    rdfs:label "Parroquia"@es ;
    rdfs:label "Parish"@en ;
    qb:dimension poa:hasParish ;
    rdfs:comment "Parroquia donde se ejecuta la actividad, usando URI de GeoNames."@es .

poa:ComponentWorkType a qb:ComponentSpecification ;
    rdfs:label "Tipo de obra"@es ;
    rdfs:label "Work type"@en ;
    qb:dimension poa:hasWorkType ;
    rdfs:comment "Clasificación de la actividad según el tipo de obra (ej. pavimentación, estudios, mantenimiento)."@es .

poa:ComponentActivityLabel a qb:ComponentSpecification ;
    rdfs:label "Nombre de la actividad presupuestaria"@es ;
    rdfs:label "Budgeted activity name"@en ;
    qb:dimension rdfs:label ;
    rdfs:comment "Etiqueta con el nombre de la actividad presupuestaria, usada para identificarla en visualizaciones."@es .

poa:ComponentLocation a qb:ComponentSpecification ;
    qb:dimension poa:hasLocation ;
    rdfs:label "Ubicación general de la actividad"@es ;
    rdfs:label "Activity location"@en ;
    rdfs:comment "Vínculo a un recurso de ubicación cuando se usa un nodo intermedio Location."@es .
###############################################################################


## Define las dimensiones y medidas ##


poa:POAStructure a qb:DataStructureDefinition ;
    rdfs:label "Estructura del conjunto de datos POA"@es ;
    qb:component
        poa:ComponentObservedActivity, 
        poa:ComponentAmount,
        poa:ComponentCode,
        poa:ComponentLocation,
        poa:ComponentWorkType,
        poa:ComponentActivityLabel,
        poa:ComponentProvince,
        poa:ComponentCanton,
